@{
    ViewBag.Title = "TfL Live Bikes";
}

@section scripts
{
    <script type="text/javascript" src="~/Scripts/Bikes/BikesAPI.js"></script>
    <script type="text/javascript" src="~/Scripts/Bikes/Index.js"></script>
    <script type="text/javascript">
        jdunkerley.bikesTfl.url = '@Url.Action("GetBikesData")';
    </script>
}

<h2>TfL Bikes</h2>

<div id="submitRow">
    <div class="col-md-12">
        <input type="submit" value="Submit To Tableau" id="submit" />
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <strong>Status: </strong>
        <span id="message"></span>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <pre id="data"></pre>
    </div>
</div>
